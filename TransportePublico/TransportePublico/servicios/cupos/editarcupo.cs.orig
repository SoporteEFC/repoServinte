        private void btnSave_Click(object sender, EventArgs e)
        {
            if (placavehiculo.Text != "" && placavehiculo.ReadOnly.Equals(false) && vehiculoAsociado != null && vehiculoAsociado.ID_VEHICULO > 0)
            {
                editarCupo();
            }
